{
  "name": "incredibowl",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.22",
    "@fortawesome/free-solid-svg-icons": "^5.10.2",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "bootstrap": "^4.3.1",
    "classnames": "^2.2.6",
    "fs-extra": "8.1.0",
    "rc-slider": "^8.6.13",
    "react": "^16.8.4",
    "react-bootstrap": "^1.0.0-beta.12",
    "react-dom": "^16.8.4",
